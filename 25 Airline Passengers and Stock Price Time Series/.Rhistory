#ARIMA + GARCH
library(quantmod)
library(timeSeries)
library(tseries)
library(forecast)
library(rugarch)
library(ecp)
getSymbols("AAPL", from="2008-02-01", to="2012-02-01")
AAPL <- Cl(AAPL)
#AAPL test
aapl_ecp <- e.divisive(X=AAPL, min.size=120)
plot(AAPL)
abline(v=.index(AAPL)[aapl_ecp$estimates[-c(1, length(aapl_ecp$estimates))]], col='blue', lwd=2)
